<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.javabucket">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FirstScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity" />
        <activity android:name=".StartingScreenActivity" />
        <activity android:name=".QuizActivity" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".OptionActivity"
            android:configChanges="orientation|keyboardHidden"/>
        <activity android:name=".InheritanceActivity"
            android:configChanges="orientation|keyboardHidden"/>
        <activity android:name=".MultithreadingActivity"
            android:configChanges="orientation|keyboardHidden"/>
        <activity android:name=".ServletActivity"
            android:configChanges="orientation|keyboardHidden"/>
        <activity android:name=".ExceptionActivity"
            android:configChanges="orientation|keyboardHidden"/>
        <activity android:name=".JDBC1Activity"
            android:configChanges="orientation|keyboardHidden"/>
        <activity android:name=".LogActivity"
            android:configChanges="orientation|keyboardHidden"/>
        <activity android:name=".Display"
            android:configChanges="orientation|keyboardHidden">

        </activity>
    </application>

</manifest>